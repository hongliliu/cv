	Author = {{Bally}, J. and {\myname{Ginsburg}}, A. and {Probst}, R. and {Reipurth}, B. and {Shirley}, Y.~L. and {Stringfellow}, G.~S.},
	Date-Added = {2014-11-05 07:15:46 +0000},
	Date-Modified = {2014-11-05 07:15:47 +0000},
	Eprint = {1409.6762},
	Journal = {\apj~accepted},
	Keywords = {Astrophysics - Solar and Stellar Astrophysics},
